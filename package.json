{
  "name": "portfolio",
  "repository": "http://github.com/drewmoore/portfolio",
  "version": "2.0.0",
  "bin": {
    "coveralls": "./node_modules/coveralls/bin/coveralls.js"
  },
  "private": true,
  "scripts": {
    "start": "DBNAME='portfolio' node app/app.js",
    "test": "DBNAME='portfolio-test' ./node_modules/mocha/bin/mocha --recursive test",
    "testwatch": "./node_modules/mocha/bin/mocha -w --recursive test",
    "coveralls": "./node_modules/mocha/bin/mocha test --recursive -r blanket -R mocha-lcov-reporter | coveralls"
  },
  "config": {
    "blanket": {
      "pattern": "/app/"
    }
  },
  "description": "Andrew Moore's web development portfolio",
  "main": "app/app.js",
  "author": "Andrew Moore",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "connect-redis": "^3.3.3",
    "express": "3.5.3",
    "express-less": "^0.1.0",
    "express-session": "^1.15.6",
    "jade": "^1.11.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mongodb": "^3.1.1",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "grunt": "^1.0.3",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-watch": "^1.1.0",
    "jshint-stylish": "^2.2.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "superagent": "^3.8.3",
    "supertest": "^3.1.0"
  },
  "engines": {
    "node": "=8.11.3"
  }
}
